NetBSD's zlib.h does not define OF().

--- src/unzip.h.orig	2004-01-21 21:50:00.000000000 +0100
+++ src/unzip.h
@@ -58,6 +58,9 @@ typedef unzFile__ *unzFile;
 typedef voidp unzFile;
 #endif
 
+#ifndef OF
+#define OF(a)	a
+#endif
 
 #define UNZ_OK                                  (0)
 #define UNZ_END_OF_LIST_OF_FILE (-100)
