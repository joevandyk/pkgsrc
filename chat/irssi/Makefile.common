# Specific distribution information.
IRSSI_VERSION=		0.8.12
IRSSI_DISTNAME=		irssi-${IRSSI_VERSION}
IRSSI_EXTRACT_SUFX=	.tar.bz2
IRSSI_DISTFILE=		${IRSSI_DISTNAME}${IRSSI_EXTRACT_SUFX}

