-        return  (int) c = *(PBYTE)((int)lpFile + vCodeOffset);
+        return  (int) (c = *(PBYTE)((int)lpFile + vCodeOffset));
     if (GotEof) return EOF;
     if (yyfp >= yypmax ) return EOF; 
-    else return (int) c = *(yyfp);
+    else return (int) (c = *(yyfp));
 }
 int PeekSecondByte()
 {
     if (GotEof) return EOF;
     if (yyfp+1 >= yypmax ) return EOF; 
-    else return (int) c = *(yyfp+1);
+    else return (int) (c = *(yyfp+1));
