@@ -1,6 +1,14 @@
+#if defined(__DragonFly__)
+#include <sys/param.h>
+#endif
+
 #include <stdlib.h>
 
 long double string_to_ld(const char *nptr, char **endptr)
 {
+#if defined(__DragonFly__) && __DragonFly_version < 200000
+	return strtod(nptr, endptr);
+#else
 	return strtold(nptr, endptr);
+#endif
 }
