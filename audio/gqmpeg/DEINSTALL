# $NetBSD: DEINSTALL,v 1.2 2003/08/30 20:22:49 jlam Exp $

case ${STAGE} in
POST-DEINSTALL)
	${CAT} << EOF
=============================================================================
If you won't be using ${PKGNAME} any longer, you may want to remove
any skins that were installed in ${PKG_PREFIX}/share/gqmpeg/skins.
=============================================================================
EOF
	;;
esac
