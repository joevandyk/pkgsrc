# usage: testcase_start <testname>
testcase_start() {
	printf "  Running testcase %s\\n" "$1"
}

# usage: assert_equal <testname> <expected> <got>
assert_equal() {
	case $2 in
	"$3")	;;
	*)	echo "[assert_equal:$1] expected \"$2\", got \"$3\"." 1>&2
		return 1;;
	esac
}
