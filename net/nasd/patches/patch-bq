+#ifndef _NASD__NASD_NETBSD_SYS_H_
+#define _NASD__NASD_NETBSD_SYS_H_
+
+#ifndef KERNEL
+#include <sys/errno.h>
+#include <sys/ioctl.h>
+#include <sys/param.h>
+#endif /* !KERNEL */
+
+typedef nasd_uint64 nasd_sys_dev_t;
+
+#define NASD_MAX_IOCTL_SIZE ((1<<_IOC_SIZEBITS)-1)
+
+#endif /* !_NASD__NASD_NETBSD_SYS_H_ */
